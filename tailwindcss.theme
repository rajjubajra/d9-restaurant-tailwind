<?php

/**
 * @file
 * Functions to support theming in the Tailwind CSS theme.
 */

function tailwindcss_page_attachments_alter(&$page){
  $path = $current_path = \Drupal::service('path.current')->getPath();
  if($path == '/node/home_page') {
    $page['#attached']['library'][] = 'tailwindcss/homepage-library';
  }
}